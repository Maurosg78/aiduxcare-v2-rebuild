{
  "name": "aiduxcare-v2",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --no-cache --config ./eslint.config.js src --ext .js,.jsx,.ts,.tsx",
    "lint:src": "eslint --no-cache --config ./eslint.config.js src --ext .js,.jsx,.ts,.tsx",
    "lint:single": "eslint --no-cache --config ./eslint.config.js",
    "lint:fix": "eslint --no-cache --config ./eslint.config.js --fix",
    "lint:fix:components": "eslint --no-cache --config ./eslint.config.js --fix src/components --ext .js,.jsx,.ts,.tsx",
    "lint:fix:services": "eslint --no-cache --config ./eslint.config.js --fix src/services --ext .js,.jsx,.ts,.tsx",
    "lint:fix:core": "eslint --no-cache --config ./eslint.config.js --fix src/core --ext .js,.jsx,.ts,.tsx",
    "lint:fix:pages": "eslint --no-cache --config ./eslint.config.js --fix src/pages --ext .js,.jsx,.ts,.tsx",
    "lint:fix:lib": "eslint --no-cache --config ./eslint.config.js --fix src/lib --ext .js,.jsx,.ts,.tsx",
    "lint:count": "eslint --no-cache --config ./eslint.config.js src --ext .js,.jsx,.ts,.tsx -f stylish | grep -E 'problems|error|warning' | head -1",
    "preview": "vite preview",
    "test": "vitest"
  },
